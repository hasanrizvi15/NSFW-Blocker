{
  "manifest_version": 2,
  "name": "NSFWBlock",
  "version": "1",
  "description": "Blocks NSFW content in webpages",
  "icons": { "128": "icon.png" },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["background"],
  "browser_action": {
    "default_title": "not",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./js/contentscript.js"]
    }
  ]
}
